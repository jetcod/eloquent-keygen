{
    "name": "jetcod/eloquent-keygen",
    "description": "A Laravel package for custom-format primary key generation in Eloquent models.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "godruoyi/php-snowflake": "^2.0|^3.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "fakerphp/faker": "^1.23"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetcod\\Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetcod\\Eloquent\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hamid Ghorashi",
            "email": "h.ghorashi@gmail.com"
        }
    ],
    "scripts": {
        "phpcs-fix": "./vendor/bin/php-cs-fixer fix ",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=-1 src/",
        "test": "./vendor/bin/phpunit --testdox "
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetcod\\Eloquent\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}

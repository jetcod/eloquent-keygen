{
    "name": "jetcod/eloquent-keygen",
    "description": "A Laravel package for custom-format primary key generation in Eloquent models.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^11.0",
        "fakerphp/faker": "^1.23"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetcod\\EloquentKeygen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetcod\\EloquentKeygen\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hamid Ghorashi",
            "email": "h.ghorashi@gmail.com"
        }
    ],
    "scripts": {
        "phpcs-fix": "php-cs-fixer fix ",
        "phpstan": "phpstan analyse --memory-limit=-1 src/",
        "test": "./vendor/bin/phpunit --testdox "
    }
}
